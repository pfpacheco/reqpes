CREATE MATERIALIZED VIEW REQPES.VM_USUARIO
REFRESH FORCE ON DEMAND
AS
SELECT USUARIO_SQ
      ,IDENTIFICACAO
      ,SENHA
      ,NIVEL
      ,EMAIL
      ,COD_UNIDADE
      ,FL_ATIVO
      ,UNIDADE
FROM   USUARIO@GERAL_WEB;
grant select, update on REQPES.VM_USUARIO to AN$LUIZ_AOBELEUTERIO;
grant select, update on REQPES.VM_USUARIO to AN$MARCOS_ABARBOSA;
grant select, insert, update, delete on REQPES.VM_USUARIO to AN$RHEV;


